		<dc:title id="title">The Swiss Family Robinson</dc:title>
		<meta property="file-as" refines="#title">Swiss Family Robinson, The</meta>
		<meta property="dcterms:alternative" refines="#title">The Family Robinson Crusoe, or, Journal of a Father Shipwrecked, with His Wife and Children, on an Uninhabited Island</meta>
		<dc:subject id="subject-1">Islands -- Fiction</dc:subject>
		<dc:subject id="subject-2">Survival -- Fiction</dc:subject>
		<dc:subject id="subject-3">Family life -- Fiction</dc:subject>
